{
    "editor.renderWhitespace": "all",
    "code-eol.newlineCharacter": "¬",
    "code-eol.returnCharacter" : "¤",
    "code-eol.crlfCharacter"   : "¤¬",
    "workbench.colorTheme": "Dracula",
    "workbench.startupEditor": "none",
    "security.workspace.trust.untrustedFiles": "open",
    "explorer.confirmDelete": false,
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "git.autofetch": true,
    "vetur.ignoreProjectWarning": true,
    "explorer.confirmDragAndDrop": false,
    "intelephense.diagnostics.undefinedTypes": false,
    "liveServer.settings.donotShowInfoMsg": true,
    "liveServer.settings.NoBrowser": true,
    "files.exclude": {
        "**/.commit*": true,
        "**/.editorconfig": true,
        "**/.prettierrc.json": true,
        "**/.styleci.yml": true,
        "**/artisan": true,
        "**/phpunit.xml": true,
        "**/server.php": true,
        "**/tests": true
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "css.styleSheets": [
        "https://cdnjs.cloudflare.com/ajax/libs/bootstrap/5.1.0/js/bootstrap.min.js"
    ],
    "[vue]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.editor.untitled.hint": "hidden",
    "task.problemMatchers.neverPrompt": {
        "shell": true
    },
    "vetur.validation.template": false,
    "intelephense.diagnostics.undefinedMethods": false,
    "task.autoDetect": "off",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.linkedEditing": true,
    "search.exclude": {
        "**/.phpstorm.meta.php": true,
        "**/js/app.js": true
    },
    "intelephense.diagnostics.undefinedFunctions": false,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "quokka.compactMessageOutput": true,
    "quokka.automaticRestart": true,
    "quokka.automaticStartRegex": "\\.(ts|js)$",
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "quokka.showCodeLensInOutputChannel": false,
    "php.suggest.basic": false,
    "php.validate.enable": false,
    "intelephense.environment.shortOpenTag": false,
    "intelephense.phpdoc.returnVoid": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.minimap.enabled": false,
    "cmake.configureOnOpen": false,
    "extensions.ignoreRecommendations": true,
    "editor.fontFamily": "'JetBrains Mono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback', 'Material Icons', 'Font Awesome 5 Free', 'Font Awesome 5 Free Solid', 'Font Awesome 5 Brands', 'JoyPixels'",
    "bracket-pair-colorizer-2.depreciation-notice": false
}